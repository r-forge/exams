<assessmentTest schemaLocation="http://www.imsglobal.org/xsd/imsqti_v2p1 imsqti_v2p1.xsd" identifier="##TestIdentifier" title="##TestTitle" xmlns="http://www.imsglobal.org/xsd/imsqti_v2p1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
 <outcomeDeclaration identifier="SCORE" cardinality="single" baseType="float">
  <defaultValue>
   <value>0</value>
  </defaultValue>
 </outcomeDeclaration>
 <outcomeDeclaration identifier="MAXSCORE" cardinality="single" baseType="float">
  <defaultValue>
   <value>
     ##TestPoints
   </value>
  </defaultValue>
 </outcomeDeclaration>
 <testPart identifier="testpartID" navigationMode="nonlinear" submissionMode="individual">

  \exinput{sections}

 </testPart>
 <outcomeProcessing>
  <setOutcomeValue identifier="SCORE">
   <sum>
    <testVariables variableIdentifier="SCORE"/>
   </sum>
  </setOutcomeValue>
 </outcomeProcessing>
</assessmentTest>

