<assessmentTest xsi:schemaLocation="http://www.imsglobal.org/xsd/imsqti_v2p1 http://www.imsglobal.org/xsd/qti/qtiv2p1/imsqti_v2p1p1.xsd" identifier="##AssessmentId##" title="##TestTitle##" xmlns="http://www.imsglobal.org/xsd/imsqti_v2p1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
<outcomeDeclaration identifier="SCORE" cardinality="single" baseType="float">
<defaultValue>
<value>##Score##</value>
</defaultValue>
</outcomeDeclaration>
<outcomeDeclaration identifier="PASS" cardinality="single" baseType="boolean">
<defaultValue>
<value>false</value>
</defaultValue>
</outcomeDeclaration>
<outcomeDeclaration identifier="FEEDBACKMODAL" cardinality="multiple" baseType="identifier" view="testConstructor"/>
<outcomeDeclaration identifier="MAXSCORE" cardinality="single" baseType="float">
<defaultValue>
<value>##MaxScore##</value>
</defaultValue>
</outcomeDeclaration>
##TimeLimits##
<testPart identifier="##TestpartId##" navigationMode="nonlinear" submissionMode="individual">
<itemSessionControl allowComment="false" maxAttempts="##MaxAttempts##" showSolution="##ShowSolution##"/>
##AssessmentSections##
</testPart>
<outcomeProcessing>
<setOutcomeValue identifier="SCORE">
<sum>
<testVariables variableIdentifier="SCORE"/>
</sum>
</setOutcomeValue>
<outcomeCondition>
<outcomeIf>
<gte>
<sum>
<testVariables variableIdentifier="SCORE"/>
</sum>
<baseValue baseType="float">##CutValue##</baseValue>
</gte>
<setOutcomeValue identifier="PASS">
<baseValue baseType="boolean">true</baseValue>
</setOutcomeValue>
</outcomeIf>
<outcomeElse>
<setOutcomeValue identifier="PASS">
<baseValue baseType="boolean">false</baseValue>
</setOutcomeValue>
</outcomeElse>
</outcomeCondition>
</outcomeProcessing>
</assessmentTest>


<assessmentSection identifier="##SectionId##" fixed="false" title="##SectionTitle##" visible="true">
<selection select="1"/>
<ordering shuffle="true"/>
<rubricBlock view="candidate">
<p>##SectionDescription##</p>
</rubricBlock>
##SectionItems##
</assessmentSection>


